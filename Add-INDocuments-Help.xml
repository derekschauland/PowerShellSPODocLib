	<command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">
		<!-- Command-->
		<command:details>
			<command:name>Add-INDocuments</command:name>
			<maml:description>
				<maml:para>Add-INDocuments will upload files from a path on a computer to the specified library within a SharePoint Online environment.</maml:para>
			</maml:description>
			<maml:copyright>
				<maml:para />
			</maml:copyright>
			<command:verb>Add</command:verb>
			<command:noun>INDocuments</command:noun>
			<dev:version />
		</command:details>
		<maml:description>
			<maml:para>Add-INDocuments will upload files from a path on a computer to the specified library within a SharePoint Online environment. You will need to be connected to SharePoint Online before uploading.</maml:para>
		</maml:description>
		<command:syntax>
			<!-- Parameter Sets-->
		</command:syntax>
		<command:parameters>
			<!-- All Parameters-->
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>Path</maml:name>
				<maml:description>
					<maml:para>The Path parameter specifies the local path to the files you wish to upload.</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false">String</command:parameterValue>
				<dev:defaultValue></dev:defaultValue>
			</command:parameter>
			<command:parameter required="false" globbing="false" pipelineInput="false" variableLength="false" position="named">
				<maml:name>Library</maml:name>
				<maml:description>
					<maml:para>The library parameter specifies the folder name of the library inside of SharePoint Online the files where the files will be uploaded.</maml:para>
				</maml:description>
				<command:parameterValue required="false" variableLength="false">String</command:parameterValue>
				<dev:defaultValue></dev:defaultValue>
			</command:parameter>
		</command:parameters>
		<command:examples>
			<!-- Examples-->
			<command:example>
				<maml:title>EXAMPLE 1</maml:title>
				<maml:introduction>
					<maml:para>This command will upload files from C:\files to the "Great Documents" Library of the https://company.sharepoint.com site</maml:para>
				</maml:introduction>
				<dev:code>C:\PS&gt; Add-INDocuments -path C:\files -Library "Great Documents" </dev:code>
				<dev:remarks>
					<maml:para />
				</dev:remarks>
			</command:example>
		</command:examples>
	</command:command>
